{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Breadth-First-search",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/dist/Algorithm/BreadthFirstSearchAlgorithm/index.js",
      "outFiles": ["${workspaceFolder}/dist/**/*.js"]
    },

    {
      "type": "node",
      "request": "launch",
      "name": "Zigzag Conversion",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/dist/Algorithm/TraversalPattern/index.js",
      "outFiles": ["${workspaceFolder}/dist/**/*.js"]
    },

    {
      "type": "node",
      "request": "launch",
      "name": "Missing_Number",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/dist/MultipleChoiceExamples/missingNumber.js",
      "outFiles": ["${workspaceFolder}/dist/**/*.js"]
    }
  ]
}
